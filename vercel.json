{
  "version": 2,
  "builds": [
    {
      "src": "serverless/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/get-properties",
      "dest": "serverless/get-properties.js"
    },
    {
      "src": "/api/add-property",
      "dest": "serverless/add-property.js"
    }
  ],
  "env": {
    "AIRTABLE_API_KEY": "@airtable_api_key",
    "AIRTABLE_BASE_ID": "@airtable_base_id"
  }
}
